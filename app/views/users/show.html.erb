<div class="row">
  <div class="small-12 columns">
    <h1><%= @user.username %> <small>(<%= @user.score %>)</small></h1>
    <% if current_user == @user %>
    <h5>Your API Key: <%= @user.api_key %></h5>
    <% end %>
    <h3 class="subheader">Recent Solutions</h3>
    <% @user.solutions.order('id DESC').take(5).each do |solution| %>
    <%= render partial: 'solutions/solution', object: solution %>
    <% end %>
  </div>
</div>
